.ranking-float u {display:block;width:20px;height:1px;position:absolute;bottom:-1px;right:35px;cursor:pointer;border-bottom:1px solid #fff;}
.ranking-float u:before,.ranking-float u:after {content:" ";display:block;width:1px;border-left:1px solid #ddd;position:absolute;}
.ranking-float u:before {top:-6px;left:10px;height:25px;
  transform:rotate(-55deg);
  -ms-transform:rotate(-65deg);
  -moz-transform:rotate(-65deg);
  -webkit-transform:rotate(-65deg);
  -o-transform:rotate(-65deg);
}
.ranking-float u:after {top:1px;right:-2px;height:11px;
  transform:rotate(-45deg);
  -ms-transform:rotate(-45deg);
  -moz-transform:rotate(-45deg);
  -webkit-transform:rotate(-45deg);
  -o-transform:rotate(-45deg);
}
