.msfk-price span {font-size:16px;}
.msfk-btn {float:right;height:45px;padding:0 20px;line-height:45px;margin:0;text-align:center;background:#e54c3d;font-size:16px;color:#fff;}

/*鑴ラ湶鑴宠劉楣胯劰纰岃尗鑴冲嵂鑴拌劗鑴瞾*/
.cont {width:100%;min-height:1%;margin-bottom:10px;border-top:1px solid #e1e1e1;background:#fff;border-bottom:1px solid #e1e1e1;}
.cont:before,.cont:after {content:" ";display:block;clear:both;}
.tzgd-author {overflow:hidden;margin:0 15px;padding:10px 0;}
.tzgd-author img {float:left;width:40px;height:40px;margin-right:10px;border-radius:50%;overflow:hidden;}
.tzgd-author h2 {font-weight:normal;font-size:16px;color:#444;}
