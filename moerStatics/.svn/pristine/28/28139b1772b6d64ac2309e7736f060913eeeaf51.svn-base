*{margin:0;padding:0;font-family:"Microsoft Yahei", "Helvetica Neue", Helvetica, Arial, sans-serif;line-height:auto;}
ul,ol,li {list-style:none;}
a img {border:0;}
a,a:hover,a:active,a:visited {text-decoration:none;color:#inherit;}

body {background:#ededed;padding-top:44px;}
.main {width:100%;overflow:hidden;position:absolute;}
input[type="number"]::-webkit-inner-spin-button,
input[type="number"]::-webkit-outer-spin-button {
  height: auto;
  -webkit-appearance: none !important;
  margin: 0; 
}
input[type="number"] {
  -moz-appearance:textfield; 
}

/*鑴瞾鑴欏繖鑴ヨ矾铏忛┐ 鎺崇湁鑴岀瘬鎺抽簱鑴滄ゼ*/
.header {width:100%;height:44px;line-height:44px;background:#1F2032;overflow:hidden;text-align:center;position:fixed;top:0;z-index:999;}
.header a {font-size:20px;color:#fff;}
.header .logo {float:left;max-height:30px;margin:7px 0 7px 20px;position:relative;z-index:10;}
.header .search {float:right;width:30px;height:40px;margin:0 15px 0 10px;background:url(../../images/header-icon.fw.png) no-repeat;background-size:90px 250px;background-position:5px -92px;cursor:pointer;position:relative;z-index:10;}
.header .header-menu {float:left;width:30px;height:40px;margin:0 15px 0 10px;background:url(../../images/header-menu.png) no-repeat;background-size:250px 250px;background-position:-14px -48px;cursor:pointer;position:relative;z-index:99;position:relative;z-index:10;}
.header .header-home {float:left;width:40px;height:40px;margin-left:10px;background:url(../../images/header-icon.fw.png) no-repeat;background-size:90px 250px;background-position:0px -39px;cursor:pointer;position:relative;z-index:10;}
.header .header-back {float:left;width:40px;height:40px;margin-left:10px;background:url(../../images/header-icon.fw.png) no-repeat;background-size:90px 250px;background-position:0px 12px;cursor:pointer;position:relative;z-index:10;}
.header span {float:left;width:100%;font-size:18px;color:#fff;position:absolute;top:0;left:0;text-align:center;z-index:1;}

/*鑴ｈ劑鑴ｆ椹磋矊*/
.search-cont {width:100%;margin-bottom:10px;padding:15px 0;background:#fff;border-bottom:1px solid #e1e1e1;display:none;position:fixed;top:44px;z-index:99;}
.search-cont .form-group {padding:0 15px;overflow:hidden;}
.search-cont input {float:left;width:75%;height:35px;margin-left:-1px;border:1px solid #e1e1e1;border-right:none;border-top-left-radius:4px;border-bottom-left-radius:4px;border-top-right-radius:0px;border-bottom-right-radius:0px;text-indent:8px;outline:none;-webkit-appearance:none;font-size:16px;}
.search-cont button {float:left;width:25%;height:37px;border:0;cursor:pointer;background:#8689c0;font-size:16px;color:#fff;border-top-right-radius:4px;border-bottom-right-radius:4px;}

/*鑴瞾鑴欏繖纰岃劤铏忛┐*/
.footer {width:100%;height:74px;}
.footer .foot {width:100%;height:40px;padding:0;background:#1f2032;}
.footer .foot a {float:left;width:25%;line-height:40px;font-size:14px;color:#fff;text-align:center;}
.footer .foot-copy {line-height:34px;text-align:center;font-size:12px;color:#666e8c;background:#fff;}

.container {margin-top:10px;}
.container:before,.container:after {content:" ";display:block;clear:both;}
.list {width:100%;margin-bottom:10px;background:#fff;border-bottom:1px solid #e1e1e1;}
.list-header {width:100%;margin:0 15px;padding:12px 0 8px 0;overflow:hidden;}
.list-header div {margin-right:35px;overflow:hidden;}
.list-header .list-thumb {float:left;width:35px;height:35px;margin-right:5px;border-radius:50%;}
.list-header h3 {line-height:15px;margin-bottom:5px;font-size:14px;color:#444;} 
.list-header h3 a {font-size:14px;color:#444;}
.list-header p {padding-left:40px;padding-right:40px;font-size:14px;line-height:20px;color:#888;white-space:nowrap;}
.list-body {padding:8px 0;margin:0 15px;border-top:1px solid #f5f5f5;}
.list-body h2 {font-size:18px;font-weight:bolder;color:#333;margin-bottom:5px;}
.list-body h2 a {color:#333;}
.list-body p {font-size:16px;color:#666;line-height:25px;}
.list-body p a {font-size:16px;color:#666;line-height:25px;text-decoration:none;}
/* 鑴ㄨ劶璧傝剾list-footer纰岃剾padding鑴﹂檵0 15;鑴鑴﹂檵0 20 */
.list-footer {height:30px;padding:0 15px;line-height:28px;border-top:1px solid #e1e1e1;background:#f9f9f9;overflow:hidden;}
.list-footer em {float:right;height:20px;margin:5px 0;padding:0 5px;border-radius:3px;line-height:20px;text-align:center;font-size:14px;font-style:normal;color:#555;background:#dfdfdf;display:none;}
.list-footer span {font-size:14px;color:#999;margin-right:5px;}
.list-footer span i {font-style:normal;}
.list-footer .read-num {float:right;position:relative;top:1px;}
.list-more {width:100%;height:40px;line-height:40px;margin-bottom:10px;text-align:center;border-bottom:1px solid #e1e1e1;background:#fff;vertical-align:middle;}
.list-more a {padding-right:25px;display:inline-block;font-size:14px;color:#333;background:url(../../images/wap_icon.png) no-repeat;background-size:50px 250px;background-position:90px -109px;}

.list-scdt .list-body {margin:0;padding:8px 15px;border-top:1px solid #e1e1e1;}
.list-scdt .list-body p {padding-left:13px;margin-bottom:10px;}
.list-scdt .list-body p:last-child {margin:0;}
.list-scdt .list-body p:before {content:" ";float:left;width:6px;height:6px;margin-top:7px;margin-left:-13px;border-radius:50%;background:#999;}
.list-scdt .list-footer em {display:block;}

/*铏忚効鍗よ劷纰岃劅鑴楀綍鑴岃祩*/
.float-block {width:100%;height:100%;position:absolute;top:0;left:0;opacity:0.1;background:#fff;display:none;z-index:80;}
.sidebar {width:212px;height:100%;position:fixed;top:0;left:-212px;background:#30314e;overflow:hidden;}
.sidebar-logout {height:15px;text-align:right;line-height:15px;position:absolute;top:55px;left:15px;}
.sidebar-logout a {font-size:12px;color:#ccc;}
.sidebar-logout i {float:right;width:22px;height:15px;margin-top:2px;margin-left:3px;background:url(../../images/sidebar-icon.png) no-repeat;background-size:50px 300px;background-position:0 -270px;}
.sidebar-thumb {width:65px;height:65px;margin:95px auto 0;border:2px solid #eee;border-radius:50%;overflow:hidden;}
.sidebar-thumb img {width:55px;height:55px;margin:5px;border-radius:50%;}
.sidebar .sidebar-name-login {text-align:center;width:178px;margin:12px auto auto;}
.sidebar .sidebar-username {margin-bottom:40px;font-weight:normal;line-height:14px;font-size:14px;color:#ededed;}
.sidebar .sidebar-username a {font-size:14px;color:#ededed;}
.sidebar .sidebar-login-btn {display:inline-block;width:100%;line-height:35px;font-size:16px;margin-bottom:40px;color:#fff;background:#e94c3e;border:1px solid #e83a2d;border-radius:5px;}
.sidebar .sidebar-nav {width:100%;}
.sidebar-nav li {width:100%;height:43px;padding-left:22px;line-height:45px;border-bottom:1px solid #282942;border-top:1px solid #3e3f64;overflow:hidden;}
.sidebar-nav li:first-child {border-top:0;}
.sidebar-nav li:last-child {border-bottom:0;}
.sidebar-nav li a {font-size:16px;color:#fff;}
.sidebar-icon {float:left;width:25px;height:45px;margin-right:15px;background:url(../../images/sidebar-icon.png) no-repeat;background-size:50px 300px;}
.sidebar-icon.sidebar-icon-1 {background-position:0 -48px;}
.sidebar-icon.sidebar-icon-2 {background-position:0 -95px;}
.sidebar-icon.sidebar-icon-3 {background-position:0 -133px;}
.sidebar-icon.sidebar-icon-4 {background-position:0 -168px;}
.sidebar-icon.sidebar-icon-5 {background-position:0 -215px;}

/*闇叉紡纰屾ゼ鑴犺矾鑴犺劎*/
