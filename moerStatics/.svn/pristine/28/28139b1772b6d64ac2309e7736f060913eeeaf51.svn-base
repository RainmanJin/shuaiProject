.dd-header h4 {font-size:14px;font-weight:normal;color:#666;}
.dd-header .color-red {color:#e84c3d;}
.dd-body {overflow:hidden;}
.dd-list li {height:48px;line-height:48px;padding:0 15px;border-bottom:1px solid #e0e0e0;font-size:16px;color:#333;cursor:pointer;overflow:hidden;}
.dd-list li:last-child {border-bottom:0;}
.dd-list .zf-icon {float:left;width:48px;height:48px;margin-right:10px;background:url(../../images/dingdan-icon.fw.png) no-repeat;background-size:50px 300px;}
.dd-list .zf-icon.zf-wx {background-position:0 12px;}
.dd-list .zf-icon.zf-zfb {background-position:0 -56px;}
.dd-list .zf-icon.zf-yhk {background-position:0 -123px;}
.dd-list .zf-icon.zf-select {width:30px;float:right;margin-right:0;background-position:10px -197px;}
.dd-list .zf-icon.zf-unselect {width:30px;float:right;margin-right:0;background-position:10px -243px;}
.dd-list-1 li {font-size:16px;}
.dd-price {float:right;width:20%;text-align:right;margin-top:12px;margin-right:20px;color:#e54c3d;}
.dd-price em {font-style:normal;font-size:12px;}
.dd-price span {font-size:14px;}
.dd-article {width:75%;margin:12px 0 12px 15px;padding-right:20px;}
.dd-article h5 {font-size:16px;margin-bottom:3px;font-weight:normal;color:#333;}
.dd-article h5 a {color:#333;}
.dd-article span {margin-right:10px;font-size:14px;color:#999;}
.dd-article span:nth-child(2) {margin-right:10px;font-size:14px;color:#666;}
.dd-yhq.dd-list li {height:60px;line-height:60px;}
.dd-yhq em {float:right;color:#e54c3d;font-style:normal;font-size:14px;position:relative;top:1px;}
.dd-yhq em i {font-style:normal;font-size:12px;}
.dd-yhq em b {font-style:normal;font-weight:normal;font-size:14px;}
.dd-yhq h5 {line-height:16px;margin-top:10px;font-size:16px;font-weight:normal;color:#333;}
.dd-yhq p {line-height:14px;font-size:12px;color:#999;margin-top:6px;}
.dd-yhq span {position:relative;top:7px;}
.dd-yhq .yhq-select .zf-select {display:block;}
.dd-yhq .yhq-select .zf-unselect {display:none;}
.dd-yhq li .zf-select {display:none;}

.dd-zffs li.dd-select .zf-select {display:block;}
.dd-zffs li.dd-select .zf-unselect {display:none;}
.dd-zffs li .zf-select {display:none;}

/*鑴楅搯鑴¤劎璧傞湶椹村嵂-纰岃劤铏忛┐楣胯劋闇茬瘬*/
.msfk-fixed {width:100%;height:45px;line-height:45px;border-top:1px solid #ddd;position:fixed;bottom:0;left:0;background:#fff;}
