/*鑴﹁剾鑴剹鑴瞾鑴欏繖*/
.article {}
.article-header {padding:20px 15px 0px 15px;}
.article-header>div {padding-bottom:10px;border-bottom:1px solid #d5d5d5;overflow:hidden;}
.article-header h1 {margin-top:-3px;margin-bottom:10px;line-height:30px;font-size:22px;font-weight:normal;color:#333;}
.article-header p {line-height:20px;font-size:14px;color:#999;overflow:hidden;}
.article-header p span {margin-right:5px;}
.article-header p i {float:right;width:20px;height:20px;line-height:20px;font-size:18px;font-style:normal;color:#333;cursor:pointer;position:relative;}
.article-header p i b.lg {font-style:normal;font-size:14px;position:absolute;top:-5px;left:9px;display:none;}
.article-header p i b.sm {font-style:normal;font-size:16px;position:absolute;top:-6px;left:11px;display:none;}
.article-header p i.font-lg .lg {display:block;}
.article-header p i.font-sm .sm {display:block;}
.article-header .article-fk {margin-top:5px;}
.article-header .article-fk span {float:right;margin-top:16px;margin-right:10px;font-size:18px;font-weight:bolder;color:#e54c3d;}
.article-header .article-fk .btn-fk {float:right;height:35px;line-height:35px;margin:10px 0 0;padding:0 15px;text-align:center;background:#e54c3d;font-size:16px;color:#fff;border-radius:3px;}
.article-header .article-fk span em {font-style:normal;font-size:16px;}
.article-header .article-fk span.help-popupspan {float:right;width:100%;display:block;height:20px;overflow:hidden;margin-top:10px;margin-right:0;font-style:normal;text-align:right;font-weight:normal;font-size:14px;color:#999;cursor:pointer;}
.article-header .article-fk span.help-popupspan:before,
.article-header .article-fk span.help-popupspan:after {content:" ";display:block;clear:both;}
.article-body {padding:0 15px;}
.article-body div {margin:10px 0 0;}
.article-body p {margin-bottom:20px;line-height:25px;font-size:16px;color:#444;overflow:hidden;}
.article-body pre {margin-bottom:20px;line-height:25px;font-size:16px;color:#444;overflow:hidden;white-space:pre-wrap;}
.article-body img {max-width:100%;}
.article-body table {width:100%;max-width:100%;margin-bottom:20px;border:1px solid #e1e1e1;
  border-spacing: 0;
  border-collapse: collapse;}
.article-body table td,
.article-body table th {padding:5px 8px;border:1px solid #e1e1e1;font-size:14px;}
.article-ds {margin-top:45px;min-height:1%;}
.article-ds-cont {margin:10px 15px 25px;}
		/* 20150527 娣诲姞鏂囩珷鏈�粓椤电殑 甯姪 */
.help-popup {display:none;width:100%;height:100%;position:fixed;top:0;left:0;background:rgba(0,0,0,.3);z-index:9999;}
.help-popup .help-cont {display:block;margin:0 15px;padding:15px;background:#fff;position:absolute;top:50%;border-radius:5px;margin-top:-50%;}
.help-cont .help-head {width:100%;padding-bottom:4px;border-bottom:1px dotted #888;position:relative;}
.help-cont .help-head h3 {font-size:18px;font-weight:normal;color:#444;}
.help-cont .help-head span {display:block;float:right;width:15px;height:15px;position:absolute;top:5px;right:0px;cursor:pointer;}
.help-cont .help-head span:before,
.help-cont .help-head span:after {content:" ";display:block;width:2px;height:13px;background:#565659;position:absolute;}
.help-cont .help-head span:before {top:10%;left:45%;
	transform:rotate(-45deg);
	-ms-transform:rotate(-45deg);   /* IE 9+ */
	-moz-transform:rotate(-45deg);  /* Firefox */
	-webkit-transform:rotate(-45deg); /* Safari 鍜�Chrome */
	-o-transform:rotate(-45deg);  /*鎭跺績鐨凮pera*/
}
.help-cont .help-head span:after {top:10%;left:45%;
	transform:rotate(45deg);
	-ms-transform:rotate(45deg);   /* IE 9+ */
	-moz-transform:rotate(45deg);  /* Firefox */
	-webkit-transform:rotate(45deg); /* Safari 鍜�Chrome */
	-o-transform:rotate(45deg);  /*鎭跺績鐨凮pera*/
}
.help-cont .help-body h4 {margin-top:13px;line-height:25px;font-weight:normal;font-size:16px;color:#E74C3D;}
.help-cont .help-body p {line-height:23px;font-weight:normal;font-size:16px;color:#545454;}
.help-cont .help-body .help-last {margin-top:13px;}
.help-cont .help-body .help-login {display:inline-block;margin:0 3px;padding:2px 7px;background:#E84C3D;color:#fff;border-radius:5px;}
.help-cont .help-body .help-logout {display:inline-block;margin:0 3px;padding:2px 7px;background:#333;color:#fff;border-radius:5px;}

/* 鑴﹂檵鑴瞾鑴欏繖纰岃剾楹撹矊鑴¤劌鎺抽簱鑴滄ゼ鑴ら搯褰曡劔璧傝祩褰曢湶鑴檵鑴ｈ劥 鍗よ労鑴欒姃椴佹皳鑴ョ 20150323 */
.article-ds .ds-btn {display:block;width:65px;height:65px;margin:0 auto;background:url(../../images/article-ds.png) no-repeat;background-size:65px 150px;background-position:0px 0px;border-radius:50%;cursor:pointer;}
.article-ds .ds-btn:hover,.article-ds .ds-btn:active {display:block;width:65px;height:65px;margin:0 auto;background:url(../../images/article-ds.png) no-repeat;background-size:65px 150px;background-position:0px bottom;border-radius:50%;cursor:pointer;}
.ds-author-list {border:1px solid #e1e1e1;position:relative;}
.ds-author-list .trig-cont {display:inline-block;width:9px;height:4px;border-bottom:1px solid #fff;position:absolute;top:-5px;left:50%;z-index:2;margin-left:-2px;}
.ds-author-list .trig-cont:before {content:" ";display:inline-block;width:7px;height:8px;border-left:1px solid #e1e1e1;position:absolute;
transform:rotate(45deg);
-ms-transform:rotate(45deg);   /* IE 9 */
-moz-transform:rotate(45deg);  /* Firefox */
-webkit-transform:rotate(45deg); /* Safari 娼炶劌 Chrome */
-o-transform:rotate(45deg);  /*闇插附鑴ㄨ剾纰岃剾Opera*/
}
.ds-author-list .trig-cont:after {content:" ";display:inline-block;width:7px;height:8px;border-right:1px solid #e1e1e1;position:absolute;
