
body {padding:0;background:#f3f3f6;}
button {outline:none;cursor:pointer;}

.wd-container{width:1180px;margin:20px auto 0;position:relative;}
.wd-container::before,.wd-container::after{content:" ";display:block;clear:both;}

.wdcont-left {float:left;width:880px;margin-right:10px;}
.wdcont-right {float:left;width:290px;}
.wdleft-cont {background:#fff;overflow:hidden;padding-bottom:20px;min-height:870px;}
.wd-zxdt-cont {background:#fff;}

/* 闂瓟tab */
.wd-tab {margin:0 2px 0;height:37px;border-bottom:3px solid #f3f3f6;}
.wd-tab ul {list-style:none;}
.wd-tab li {float:left;margin:0 10px;height:37px;line-height:40px;border-bottom:3px solid #f3f3f6;color:#666;}
.wd-tab li:first-child {margin-left:40px;}
.wd-tab li.tab-select,.wd-tab li:hover,.wd-tab li:active {border-bottom-color:#03a2d0;color:#03a2d0;}
.wd-tab span {float:right;margin-right:45px;line-height:39px;font-size:14px;color:#666;}
.wd-tab span em,.wd-tab span a {margin:0 5px;font-size:inherit;font-style:normal;color:#00a9da;}
.wd-tab li a {padding:0 10px;font-size:16px;color:inherit;}
.wd-tab li a span {float:none;padding:0;margin:0;color:inherit;}
.wd-tab .wdtab-linkright {float:right;margin-right:45px;line-height:39px;font-size:14px;color:#666;text-decoration:none;}
.wd-tab .wdtab-linkright em,.wd-tab .wdtab-linkright span {float:none;margin:0 5px;font-size:inherit;font-style:normal;color:#00a9da;text-decoration:none;}

/* 闂瓟 鏈�柊鍔ㄦ� 鎺ㄨ崘 */
.wd-recommend {width:760px;margin:20px auto 0;padding:10px 0 20px;background:#f8f8f9;}
.wd-recommend::before,.wd-recommend::after{content:" ";display:block;clear:both;}
.wd-recommend h1 {font-size:16px;font-weight:normal;color:#666;text-align:center;}
.wd-recommend h1 span,.wd-recommend h1 a {margin-left:10px;font-size:14px;color:#3f79a5;}
.wd-recommend-body {width:720px;margin-top:10px;margin-left:15px;}
.wd-recommend-list {float:left;width:208px;margin-left:10px;padding:10px;border:1px solid #e3e3eb;background:#fff;}
.wd-recommend-author {padding-bottom:6px;margin-bottom:6px;border-bottom:1px solid #e7e7e8;}
.wd-recommend-author img {float:left;width:40px;height:40px;margin-right:7px;border-radius:50%;}
.wd-recommend-author h3 {font-size:14px;color:#464646;}
.wd-recommend-author h4 {font-size:12px;color:#6b7b8e;}
.wd-recommend-author h4 span {margin:0 4px 8px;font-weight:normal;}
.wd-recommend-author p {width:100%;margin-top:15px;font-size:12px;color:#8c9fb6;overflow:hidden;}
.wd-recommend-author p span {font-weight:bolder;color:#e54c3d;}
.wd-recommend-atten {}
.wd-recommend-atten p {font-size:12px;color:#8c9fb6;}
.wd-recommend-atten button {margin-top:4px;padding:3px 10px;font-size:12px;color:#fff;background:#00a53c;border:none;border-radius:4px;outline:none;cursor:pointer;}

.have-dynamic {width:758px;height:32px;line-height:32px;margin:20px auto 5px;text-align:center;border:1px solid #dbecf1;background:#f2f8fa;font-size:14px;color:#666;cursor:pointer;}
.have-dynamic a {float:left;width:100%;height:100%;text-align:center;line-height:32px;font-size:14px;color:#666;cursor:pointer;}

/* 闂瓟鍒楄〃 鍙�闂瓟鍥炲 */
.wd-list-cont {width:760px;margin:0 auto;}
.wd-list {padding:25px 0 15px;border-bottom:1px solid #e8e8e8;}
.wd-list-cont .wd-list:last-child {border:0;}
.wd-list::before,.wd-list::after {content:" ";display:block;clear:both;}
.wdlist-avatar {float:left;width:50px;margin-right:10px;}
.wdlist-avatar img {width:40px;height:40px;margin-left:5px;border-radius:50%;}
.wdlist-avatar span {display:block;margin:10px auto 0;padding:3px 0;text-align:center;font-size:12px;font-weight:bolder;color:#03a2d0;background:#f2f8fa;border-radius:4px;cursor:pointer;}
.wdlist-article {float:left;width:700px;}
.wdlist-article-cont::before,.wdlist-article-cont::after {content:" ";display:block;clear:both;}
.wdlist-article-cont h2 {font-size:12px;color:#aab2bd;font-weight:normal;}
.wdlist-article-cont h2 em {float:right;color:#aab2bd;font-style:normal;}
.wdlist-article-cont h2 span {margin-right:5px;color:#3f79a5;}
.wdlist-article-cont h1 {margin:5px 0 6px;font-size:16px;font-weight:bolder;color:#333;}
.wdlist-article-cont h1:hover,.wdlist-article-cont h1:active {color:#03a2d0;}
.wdlist-article-cont h1 a {color:inherit;}
.wdlist-article-cont p {font-size:14px;color:#555;line-height:22px;}
.wdlist-article-cont p img {}
.wdlist-article-cont p img.img-sm {float:left;margin-right:10px;max-width:160px;max-height:100px;}
.wdlist-article-cont p a {margin-left:15px;font-size:14px;color:#3f79a5;line-height:22px;}
.wdlist-article-cont .wdcontrol .control-hover {display:none;}
.wdlist-article-cont:hover .wdcontrol .control-hover {display:inline-block;}

/* 璇勮 */
.wdlist-comments {margin-top:10px;padding:0 15px 20px;border:1px solid #dfdfdf;position:relative;}
.wdlist-comments u {float:right;width:13px;height:8px;position:absolute;top:-9px;left:130px;border-bottom:1px solid #fff;}
.wdlist-comments u::before,
.wdlist-comments u::after {content:" ";display:block;width:1px;height:10px;background:#dfdfdf;position:absolute;}
.wdlist-comments u::before {top:0;left:3px;
	transform:rotate(45deg);
	-ms-transform:rotate(45deg);   /* IE 9+ */
	-moz-transform:rotate(45deg);  /* Firefox */
	-webkit-transform:rotate(45deg); /* Safari 闁告粣鎷稢hrome */
	-o-transform:rotate(45deg);  /*闁诡叀娉涚缓楣冩儍閸戠晝era*/
}
.wdlist-comments u::after {top:0;right:3px;
	transform:rotate(-45deg);
	-ms-transform:rotate(-45deg);   /* IE 9+ */
	-moz-transform:rotate(-45deg);  /* Firefox */
	-webkit-transform:rotate(-45deg); /* Safari 闁告粣鎷稢hrome */
	-o-transform:rotate(-45deg);  /*闁诡叀娉涚缓楣冩儍閸戠晝era*/
}
.wdcomments-list {padding:15px 0 10px;overflow:hidden;border-bottom:1px solid #e8e8e8;}
.wdlist-comments .wdcomments-list:last-child {border:0;}
.wdcomments-avatar {float:left;width:40px;}
.wdcomments-avatar img {width:30px;height:30px;border-radius:50%;}
