.wdcomments-cont::before,.wdcomments-cont::after {content:" ";display:block;clear:both;}
.wdcomments-cont h4 {font-size:12px;color:#3f79a5;}
.wdcomments-cont h4 a {font-size:inherit;color:inherit;}
.wdcomments-cont h4 span {margin:0 5px;font-size:inherit;font-weight:normal;color:#a8afb9;}
.wdcomments-list .wdcomments-cont p {font-size:12px;color:#555;line-height:20px;}
.wdcomments-cont .wdcomments-control {font-size:12px;color:#aab2bd;overflow:hidden;}
.wdcomments-cont .wdcomments-control::before,.wdcomments-cont .wdcomments-control::after {content:" ";display:block;clear:both;}
.wdcomments-cont .wdcomments-control h5 {float:right;font-weight:normal;cursor:pointer;}
.wdcomments-cont .wdcomments-control h5 em {margin-right:4px;font-style:normal;font-weight:bolder;}
.wdcomments-cont .wdcomments-control span {margin-right:10px;cursor:pointer;}
.wdcomments-cont .wdcomments-control span em {text-decoration:none;font-style:normal;}
.wdcomments-cont .wdcomments-control span i {}
.wdcomments-text {width:100%;margin:0 auto 5px;padding-top:10px;overflow:hidden;}
.wdcomments-text textarea {width:605px;min-height:35px;padding:10px;margin-bottom:5px;font-size:12px;color:#333;line-height:20px;border:1px solid #cfcfcf;box-shadow:inset 0 0 3px 2px #f4f4f4;resize:none;}
