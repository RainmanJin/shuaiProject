.wdright-banner {width:290px;margin-bottom:10px;background:#fff;border-radius:4px;overflow:hidden;position:relative;}
.wdright-banner img {width:100%;}
.wdright-banner i {float:right;width:13px;height:13px;position:absolute;top:5px;right:5px;border-bottom:1px solid #fff;}
.wdright-banner i::before,
.wdright-banner i::after {content:" ";display:block;width:2px;height:14px;background:#acb3bd;position:absolute;}
.wdright-banner i::before {top:0;left:43%;
	transform:rotate(45deg);
	-ms-transform:rotate(45deg);   /* IE 9+ */
	-moz-transform:rotate(45deg);  /* Firefox */
	-webkit-transform:rotate(45deg); /* Safari 闁告粣鎷稢hrome */
	-o-transform:rotate(45deg);  /*闁诡叀娉涚缓楣冩儍閸戠晝era*/
}
.wdright-banner i::after {top:0;right:45%;
	transform:rotate(-45deg);
	-ms-transform:rotate(-45deg);   /* IE 9+ */
	-moz-transform:rotate(-45deg);  /* Firefox */
	-webkit-transform:rotate(-45deg); /* Safari 闁告粣鎷稢hrome */
	-o-transform:rotate(-45deg);  /*闁诡叀娉涚缓楣冩儍閸戠晝era*/
}
.wdright-banner:hover i,
.wdright-banner:active i {cursor:pointer;}
.wdright-banner i:hover::before,
.wdright-banner i:hover::after,
.wdright-banner i:active::before,
.wdright-banner i:active::after {background:#03a2d0;}

