/** 邀请回答 **/
.wdfinal-invitation {margin:8px 0 0;border:1px solid #dfdfdf;background:#fff;position:relative;}
.wdfinal-invitation u {float:right;width:13px;height:8px;position:absolute;top:-9px;left:130px;border-bottom:1px solid #fff;}
.wdfinal-invitation u::before,
.wdfinal-invitation u::after {content:" ";display:block;width:1px;height:10px;background:#dfdfdf;position:absolute;}
.wdfinal-invitation u::before {top:0;left:3px;
	transform:rotate(45deg);
	-ms-transform:rotate(45deg);   /* IE 9+ */
	-moz-transform:rotate(45deg);  /* Firefox */
	-webkit-transform:rotate(45deg); /* Safari 闁告粣鎷稢hrome */
	-o-transform:rotate(45deg);  /*闁诡叀娉涚缓楣冩儍閸戠晝era*/
}
.wdfinal-invitation u::after {top:0;right:3px;
	transform:rotate(-45deg);
	-ms-transform:rotate(-45deg);   /* IE 9+ */
	-moz-transform:rotate(-45deg);  /* Firefox */
	-webkit-transform:rotate(-45deg); /* Safari 闁告粣鎷稢hrome */
	-o-transform:rotate(-45deg);  /*闁诡叀娉涚缓楣冩儍閸戠晝era*/
}
